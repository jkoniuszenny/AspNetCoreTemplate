{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Token": {
    "SecretKey": "YourSecretKey",
    "RefreshKey": "YourRefreshKey",
    "AudienceKey": "YourAudienceKey",
    "IssuerKey": "YourIssuerKey"
  },
  "Database": {
    "ConnectionString": "Server=; Database=; User Id=; Password="
  },
  "ResponseTime": {
    "Enabled": true,
    "MilisecondsElapsedToNotify": 5000
  }
}
